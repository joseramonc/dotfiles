"*":
  "autocomplete-plus":
    autoActivationDelay: 500
    backspaceTriggersAutocomplete: true
    confirmCompletion: "enter"
    extraWordCharacters: "."
  core:
    closeEmptyWindows: false
    disabledPackages: [
      "background-tips"
      "autosave"
      "metrics"
      "spell-check"
      "welcome"
      "language-xml"
      "language-toml"
      "language-source"
      "language-property-list"
      "language-perl"
      "language-mustache"
      "language-make"
      "language-java"
      "language-go"
      "language-gfm"
      "language-clojure"
      "exception-reporting"
      "autoflow"
      "timecop"
      "bookmarks"
      "language-objective-c"
      "split-diff"
      "language-c"
      "language-git"
      "language-json"
      "autocomplete-atom-api"
      "encoding-selector"
      "package-generator"
      "styleguide"
      "language-typescript"
      "language-csharp"
      "update-package-dependencies"
      "markdown-preview"
      "language-python"
      "language-php"
      "language-sql"
      "language-shellscript"
      "language-rust-bundled"
      "about"
      "language-ruby-on-rails"
    ]
    ignoredNames: [
      ".bundle"
      ".git"
      ".DS_Store"
    ]
    openEmptyEditorOnStart: false
    packagesWithKeymapsDisabled: [
      "emmet"
      "autocomplete-atom-api"
      "git-diff"
      "rails-transporter"
      "archive-view"
      "go-to-line"
      "rails-latest-migration"
    ]
    projectHome: "/Users/joseramonc/src"
    restorePreviousWindowsOnStart: "no"
    telemetryConsent: "no"
    themes: [
      "one-dark-ui"
      "solarized-dark-syntax"
    ]
    useTreeSitterParsers: false
  editor:
    autoIndentOnPaste: true
    fontFamily: "Menlo"
    fontSize: 15
    invisibles: {}
    showIndentGuide: true
  emmet: {}
  "exception-reporting":
    userId: "f6992024-6e66-279c-64e4-816ff0307646"
  "find-and-replace": {}
  "fuzzy-finder":
    prefillFromSelection: true
    preserveLastSearch: true
    scoringSystem: "fast"
    useRipGrep: true
  "git-diff":
    showIconsInEditorGutter: true
  "markdown-preview":
    grammars: [
      "source.gfm"
      "source.litcoffee"
      "text.plain"
      "text.plain.null-grammar"
    ]
  "rails-partials": {}
  "rails-transporter": {}
  "selector-to-tag":
    activeForFileTypes: [
      ".html"
      ".erb"
    ]
  "spell-check":
    grammars: [
      "source.gfm"
      "text.git-commit"
      "text.plain"
    ]
  "split-diff": {}
  tablr:
    csvEditor: {}
    defaultColumnNamingMethod: "numericZeroBased"
  "tabs-to-spaces":
    onSave: "untabify"
  "tree-view":
    alwaysOpenExisting: true
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
    showOnRightSide: true
    sortFoldersBeforeFiles: false
  welcome:
    showOnStartup: false

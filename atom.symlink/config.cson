"*":
  "autocomplete-plus":
    autoActivationDelay: 500
    backspaceTriggersAutocomplete: true
    confirmCompletion: "enter"
  core:
    closeEmptyWindows: false
    disabledPackages: [
      "background-tips"
      "autosave"
      "metrics"
      "spell-check"
      "welcome"
      "language-xml"
      "language-toml"
      "language-source"
      "language-property-list"
      "language-perl"
      "language-mustache"
      "language-make"
      "language-java"
      "language-go"
      "language-gfm"
      "language-clojure"
      "exception-reporting"
      "autoflow"
      "timecop"
      "bookmarks"
      "language-csharp"
      "language-objective-c"
      "split-diff"
      "link"
      "markdown-preview"
      "language-c"
      "language-git"
      "language-json"
      "language-php"
      "language-python"
    ]
    ignoredNames: [
      ".bundle"
      ".git"
      ".DS_Store"
    ]
    openEmptyEditorOnStart: false
    packagesWithKeymapsDisabled: [
      "emmet"
      "autocomplete-atom-api"
      "git-diff"
    ]
    projectHome: "/Users/joseramonc/src"
    themes: [
      "one-dark-ui"
      "solarized-dark-syntax"
    ]
  editor:
    autoIndentOnPaste: true
    fontFamily: "Menlo"
    fontSize: 15
    invisibles: {}
    showIndentGuide: true
  emmet: {}
  "exception-reporting":
    userId: "f6992024-6e66-279c-64e4-816ff0307646"
  "git-diff":
    showIconsInEditorGutter: true
  "markdown-preview":
    grammars: [
      "source.gfm"
      "source.litcoffee"
      "text.plain"
      "text.plain.null-grammar"
    ]
  "rails-partials": {}
  "selector-to-tag":
    activeForFileTypes: [
      ".html"
      ".erb"
    ]
  "spell-check":
    grammars: [
      "source.gfm"
      "text.git-commit"
      "text.plain"
    ]
  "split-diff": {}
  "tabs-to-spaces":
    onSave: "untabify"
  "tree-view":
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
    showOnRightSide: true
  welcome:
    showOnStartup: false
